        <top>myst</top>
        <apply>apply</apply>
        <car>applyC</car>
        <description>IS Primary Apply Record</description>
        <command name="gems::ttApdCtrl">
            <description>Canopus APDs control</description>
            <parameter name="apd1Cmd">
                <channel>ttApdCtrl.A</channel>
                <type>STRING</type>
                <description>APD1 command (START|STOP)</description>
            </parameter>
            <parameter name="apd2Cmd">
                <channel>ttApdCtrl.B</channel>
                <type>STRING</type>
                <description>APD2 command (START|STOP)</description>
            </parameter>
            <parameter name="apd3Cmd">
                <channel>ttApdCtrl.C</channel>
                <type>STRING</type>
                <description>APD3 command (START|STOP)</description>
            </parameter>
        </command>
        <command name="gems::seqLoopCtrl">
            <description>Loop control for Seqexec</description>
            <parameter name="cmd">
                <channel>seqLoopCtrl.A</channel>
                <type>STRING</type>
                <description>Command (START|PAUSE|RESUME|STOP)</description>
            </parameter>
            <parameter name="reasons">
                <channel>seqLoopCtrl.B</channel>
                <type>STRING</type>
                <description>Dither/Filter/Sky condition</description>
            </parameter>
        </command>
    </Apply>
